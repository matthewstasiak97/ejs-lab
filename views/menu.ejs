<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Menu</title>
  </head>
  <body>
    <h1>Full Menu</h1>
    <h2>Mains</h2>
    <% menu.forEach(item => { %> <% if (item.category === "mains") { %>
    <div>
      <h3><%= item.name %> - $<%= item.price.toFixed(2) %></h3>
      <p><%= item.details %></p>
      <p>Rating: <%= item.rating %>/5</p>
    </div>
    <% } %> <% }) %>
    <h2>Desserts</h2>
    <% menu.forEach(item => { %> <% if (item.category === "desserts") { %>
    <div>
      <h3><%= item.name %> - $<%= item.price.toFixed(2) %></h3>
      <p><%= item.details %></p>
      <p>Rating: <%= item.rating %>/5</p>
    </div>
    <% } %> <% }) %>
    <h2>Sides</h2>
    <% menu.forEach(item => { %> <% if (item.category === "sides") { %>
    <div>
      <h3><%= item.name %> - $<%= item.price.toFixed(2) %></h3>
      <p><%= item.details %></p>
      <p>Rating: <%= item.rating %>/5</p>
    </div>
    <% } %> <% }) %>
  </body>
</html>
